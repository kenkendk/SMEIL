type tdata: { val: i32; };

proc plusone(in inbus)
    bus plusout {
       val: i32 = 0;
    };
{
    trace("Wrote value {}", inbus.val);
    plusout.val = inbus.val + 1;
}

network plusone_net(in source: tdata, out dest: tdata) {
    instance plusone_inst of plusone(source);
    connect 
        plusone_inst.plusout -> dest;
}
